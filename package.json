{
  "name": "feednana",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "ESLINT_NO_DEV_ERRORS=true next build",
    "build:skip-lint": "ESLINT_NO_DEV_ERRORS=true next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:reset-sequences": "npx prisma db execute --file <(echo \"SELECT setval('users_id_seq', COALESCE((SELECT MAX(id) FROM users), 0) + 1); SELECT setval('files_id_seq', COALESCE((SELECT MAX(id) FROM files), 0) + 1); SELECT setval('comments_id_seq', COALESCE((SELECT MAX(id) FROM comments), 0) + 1); SELECT setval('albums_id_seq', COALESCE((SELECT MAX(id) FROM albums), 0) + 1); SELECT setval('timelines_id_seq', COALESCE((SELECT MAX(id) FROM timelines), 0) + 1);\")"
  },
